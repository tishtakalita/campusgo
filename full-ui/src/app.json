      "supportsTablet": true,
      "bundleIdentifier": "com.studentai.platform"
    },
    "android": {
      "adaptiveIcon": {
        "backgroundColor": "#111827"
      },
      "package": "com.studentai.platform"
    },
    "web": {
      "bundler": "metro"
    }
  }
}